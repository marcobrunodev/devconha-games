scripts:
  - name: multiplayer
    code: >-
      let isJoinMatchQueryParam = false

      let myPresence= {
          session_id: '',
          user_id: '',
          username: ''
      }

      const room = {
          players: [],
          matchId: undefined,
          joinSelf: function (session_id) {
              this.players = {...this.players, [session_id]: 'self' }
          },
          join: function(session_id, cubicAnimal = undefined) {
              if (session_id != myPresence.session_id) {
                  this.players = {...this.players, [session_id]: spawnPlayer(cubicAnimal)}
              }
          },
          leave: function(session_id, cubicAnimal = undefined) {
              if (this.players[session_id] != undefined) {
                  this.players[session_id].kill = true
                  delete this.players[session_id]
              }
          }
      }


      Object.defineProperty(room, 'matchId', {
          enumerable: true,
          configurable: true,
          get: function () {
              return this._matchId
          },
          set: function (value) {
              if (!this._matchId) {
                  this._matchId = value
              }
          }
      })


      async function createMatch() {
          const { match_id: matchId, self } = await Nakama.socket.createMatch()
          myPresence = self
          room.matchId = matchId
          room.joinSelf(self.session_id)
          Nakama.socket.onmatchpresence = handleOnmatchpresence
          console.log('createMatch', matchId)
      }


      async function joinMatch(matchId) {
          const { presences, self } = await Nakama.socket.joinMatch(matchId)
          myPresence = self
          room.matchId = matchId
          room.joinSelf(self.session_id)
          presences.forEach((presence) => room.join(presence.session_id))
          Nakama.socket.onmatchpresence = handleOnmatchpresence

          return true
      }


      function spawnPlayer(cubicAnimal = Nakama.state.player.cubicAnimal, x =
      ct.random.range(20, 460), y = ct.random.range(20, 250)) {
          return ct.templates.copy(cubicAnimal, x, y)
      }


      function handleOnmatchpresence(presences) {
          presences.joins?.forEach(({session_id}) => {
              room.join(session_id)
          })
          
          presences.leaves?.forEach(({session_id}) => {
              room.leave(session_id)
          })
      }
