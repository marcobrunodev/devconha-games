      - eventKey: OnDraw
        code: |-
          Nakama.socket.onmatchdata = updateAnimation

          function updateAnimation(result) {
              const {presence: {session_id}, data: {x, y, animation, directionX}} = result

              if (x != undefined && y != undefined) {
                  room.players[session_id].x = x
                  room.players[session_id].y = y
              }

              if (animation == 'walk') {
                  room.players[session_id].$walk()
              }

              if (animation == 'idle') {
                  room.players[session_id].$idle()
              }

              if (directionX == 'right') {
                  room.players[session_id].scale.x = -1
              }

              if (directionX == 'left') {
                  room.players[session_id].scale.x = 1
              }
          }
        arguments: {}
        lib: core
    extends:
      alpha: 0.4
