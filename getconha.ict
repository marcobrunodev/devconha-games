templates:
  - type: template
    name: CubicAraraAzul
    depth: 0
    texture: 4df6fzrm1Mzcb1
    playAnimationOnStart: false
    loopAnimation: true
    animationFPS: 30
    visible: true
    events:
      - eventKey: OnActionDown
        code: |
          if (this.y > this.stage.top) {
              this.y -= this.$velocity * ct.delta
              this.$walk()
          }
        arguments:
          action: MoveUp
        lib: core
      - eventKey: OnActionDown
        code: |-
          if (this.y < this.stage.bottom ) {
              this.y += this.$velocity * ct.delta
              this.$walk()
          }
        arguments:
          action: MoveDown
        lib: core
      - eventKey: OnCreate
        code: |
          this.$velocity = 1
          this.$maxFramerate = 60
          this.opCodes = {
              walk: 1,
              idle: 2,
              directionX: 3
          }
          this.isJoin = false

          this.stage = {
              left: this.width / 2,
              right: ct.camera.width - (this.width / 2),
              top: this.height / 2,
              bottom: ct.camera.height - (this.height / 2)
          }

          console.log('stage', this.stage)

          this.$walk = () => {
              const {x, y} = this.transform.position
              this.tex = 'walk'
              this.animationSpeed = 8/this.$maxFramerate

              if(room?.matchId) {
                  Nakama.socket?.sendMatchState(room.matchId, this.opCodes.walk, {x, y, animation: 'walk'})
              }
          }

          this.$idle = () => {
              const {x, y} = this.transform.position
              this.tex = 'idle'
              this.animationSpeed = 2/this.$maxFramerate
              
              if (room?.matchId) {
                  Nakama.socket?.sendMatchState(room.matchId, this.opCodes.idle, {x ,y, animation: 'idle'})
              }
          }

          this.$idle()
          this.play()

          Nakama.state.player = {
              cubicAnimal: 'CubicAraraAzulMultiplayer'
          }
        arguments: {}
        lib: core
      - eventKey: OnActionDown
        code: |-
          if (this.x < this.stage.right) {
              this.x += this.$velocity * ct.delta
              this.$walk()
              this.scale.set(-1, 1)

              Nakama.socket.sendMatchState(room.matchId, this.opCodes.directionX, {directionX: 'right'})
          }
        arguments:
          action: MoveRight
        lib: core
      - eventKey: OnActionDown
        code: |-
          if (this.x > this.stage.left) {
              this.x -= this.$velocity * ct.delta
              this.$walk()
              this.scale.set(1, 1)

              Nakama.socket.sendMatchState(room.matchId, this.opCodes.directionX, {directionX: 'left'})
          }
        arguments:
          action: MoveLeft
        lib: core
      - eventKey: OnActionRelease
        code: this.$idle()
        arguments:
          action: Idle
        lib: core
    extends: {}
